{
  "name": "patrick-desjardins-blog",
  "version": "1.0.0",
  "private": true,
  "description": "Patrick Desjardins Blog",
  "author": "Patrick Desjardins",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "NODE_OPTIONS='--openssl-legacy-provider --max_old_space_size=8192' && gatsby develop",
    "debug": "node --openssl-legacy-provider --nolazy node_modules/.bin/gatsby develop --inspect-brk",
    "start": "NODE_OPTIONS='--openssl-legacy-provider' && gatsby develop",
    "build": "NODE_OPTIONS='--openssl-legacy-provider --max_old_space_size=8192' && gatsby build",
    "build:verbose": "NODE_OPTIONS='--openssl-legacy-provider --max_old_space_size=8192' && gatsby build --verbose",
    "serve": "NODE_OPTIONS='--openssl-legacy-provider' && gatsby serve",
    "clean": "gatsby clean",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js",
    "lint:mdx:single": "eslint ./blog/2017/typescript-vs-flow-part-2-of-3-high-level-coding.mdx",
    "lint:mdx": "eslint . --ext mdx"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.5.13",
    "@fontsource/oswald": "^4.5.11",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mdx-js/mdx": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "babel-plugin-styled-components": "^2.0.7",
    "clsx": "^1.2.1",
    "gatsby": "^5.2.0",
    "gatsby-plugin-feed-mdx": "^1.1.0",
    "gatsby-plugin-ffmpeg": "^0.6.0",
    "gatsby-plugin-google-analytics": "^5.2.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-mdx": "^5.2.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-plugin-styled-components": "^6.2.0",
    "gatsby-remark-copy-linked-files": "^6.2.0",
    "gatsby-remark-gifs": "^1.1.0",
    "gatsby-remark-gifs-to-videos": "^1.0.0-beta.5",
    "gatsby-remark-images": "^7.2.0",
    "gatsby-remark-prismjs": "^7.2.0",
    "gatsby-remark-static-images": "^1.2.1",
    "gatsby-remark-video": "^1.2.5",
    "gatsby-remark-videos": "^0.5.2",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-transformer-remark": "^6.2.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "mdx-embed": "^1.1.2",
    "prism-react-renderer": "^1.3.5",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "eslint": "^8.29.0",
    "eslint-plugin-mdx": "^2.0.5",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
